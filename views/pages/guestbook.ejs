<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>
    <div class="jumbotron">
        <table>
            <% guestbook.forEach(function(user){ %>
                <tr>
                <td>User: <%= user.username %></td></tr>
                <td>From: <%= user.country %></td></tr>
                <tr>
                <td><i><%= user.date %></i></td></tr>
                <tr>
                <td class="guestmessage"><strong>> <%= user.message %></strong></td>
                </tr>
           <% })%>
        </table>
    </div>

</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>